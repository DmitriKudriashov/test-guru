<h3> Show test:  <%= @test.title %> </h3>
<p>
  <strong>Level:</strong>
  <%= test_level(@test) %>
</p>

<p>
  <strong>Category:</strong>
  <%= @test.category.title %>
</p>

<p>
  <strong>Author:</strong>
  <%= @test.author.email %>
</p>

<%= link_to 'Edit Test', edit_admin_test_path(@test) %>
<br>
<p class="style_gray">
  <%= link_to 'Back to All Tests', admin_tests_path %>
</p>
<br>
<h3>Questions</h3>
 <%#= render 'questions/list', test: @test %>
<ol>
  <%= render partial: 'questions/question', collection: @test.questions %>
</ol>
