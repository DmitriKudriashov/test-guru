<% if test.questions.present? %>
  <table>
    <tr>
      <th class = "celltab100center">Question</th>
      <th class = "celltab30center">Answers</th>
       <th colspan="3">Actions</th>
    </tr>

    <%= render test.questions %>

  </table>

<% else %>
  <p>There is no questions available.</p>
<% end %>

<p>
  <%= link_to 'New question', new_admin_test_question_path(test) %>
</p>


