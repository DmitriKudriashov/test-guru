<h1>Questions</h1>
<table>
  <thead>
    <tr>
      <th colspan="5" class = "celltab30">id </th>
      <th colspan="5" class = "celltab100">QUESTIONS </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questions.each do |question| %>
      <tr>
        <td colspan="5" class = "celltab30"><%= question.id %></td>
        <td colspan="5" class = "celltab100left"><%= question.body %></td>
      </tr>
    <% end %>
  </tbody>
</table>
